<table class="table datatable with-default-setting">
  <thead>
    <tr>
      <th>Currency</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    <% @wallets.each do |wallet| %>
      <tr>
        <td><%= wallet.currency.upcase %></td>
        <td><%= wallet.amount %></td>
      </tr>
    <% end %>
  </tbody>
</table>
